<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5level %X{prefix}%msg%X{suffix}%n</pattern>
        </encoder>
    </appender>

    <logger name="org.javers" level="WARN"/>
    <logger name="org.drools" level="ERROR" />

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>